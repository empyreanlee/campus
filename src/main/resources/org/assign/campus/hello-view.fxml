<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="399.0" prefWidth="325.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.assign.campus.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" prefHeight="19.0" prefWidth="48.0" text="Hello!" />
    <TextField fx:id="name" prefHeight="25.0" prefWidth="253.0" promptText="name" />
    <TextField fx:id="email" prefHeight="25.0" prefWidth="253.0" promptText="email" />
    <TextField fx:id="password" prefHeight="25.0" prefWidth="253.0" promptText="password" />
   <TextField fx:id="confpassword" layoutX="30.0" layoutY="202.0" prefHeight="25.0" prefWidth="253.0" promptText="confirm password" />
    <Label fx:id="textfield" />
    <Label fx:id="text" text="Touch me" />

   <Button fx:id="submit" mnemonicParsing="false" onAction="#onButtonClick" text="Button" />
</VBox>
