<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="462.0" prefWidth="419.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.assign.campus.HelloController">
   <children>
      <VBox alignment="CENTER" layoutX="25.0" layoutY="9.0" prefHeight="381.0" prefWidth="347.0" spacing="20.0">
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome ">
            <font>
               <Font size="14.0" />
            </font>
         </Text>
         <ComboBox fx:id="role" prefHeight="8.0" prefWidth="80.0" promptText="Student" style="-fx-background-color: skyblue;" />
          <TextField fx:id="name" prefHeight="25.0" prefWidth="253.0" promptText="name" />
          <TextField fx:id="email" prefHeight="25.0" prefWidth="253.0" promptText="email" />
          <TextField fx:id="regNo" layoutX="30.0" layoutY="157.0" prefHeight="25.0" prefWidth="253.0" promptText="registration no" />
          <TextField fx:id="password" prefHeight="25.0" prefWidth="253.0" promptText="password" />
          <TextField fx:id="confpassword" layoutX="30.0" layoutY="202.0" prefHeight="25.0" prefWidth="253.0" promptText="confirm password" />
      
          <Button fx:id="submit" mnemonicParsing="false" onAction="#onButtonClick" style="-fx-background-color: skyblue;" text="Sign Up">
            <font>
               <Font name="Tahoma" size="13.0" />
            </font>
         </Button>
      </VBox>
      <Text layoutX="179.0" layoutY="411.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Already signed in?" wrappingWidth="116.9375" />
      <Button fx:id="signIn" layoutX="296.0" layoutY="395.0" mnemonicParsing="false" onAction="#onBtnClick" prefHeight="17.0" prefWidth="54.0" style="-fx-background-color: skyblue;" text="Sign In">
         <font>
            <Font name="Tahoma" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
